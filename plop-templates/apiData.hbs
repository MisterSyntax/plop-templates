// Reducers
export function Reducer(state = {}, action = {}) {
    switch (action.type) {
        case LOAD: {
            return state;
        }

        case LOAD_SUCCESS: {
            const newState = deep(action, 'result');
            return deepEqual(newState, state) ? state : newState;
        }

        case LOAD_FAILURE: {
            return state;
        }

        default:
            return state;
    }
}


